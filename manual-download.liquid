<!-- Section Start -->
<div class="multitab-sections" style="padding-top: {{ section.settings.padding_top }}px; padding-bottom: {{ section.settings.padding_bottom }}px;">
    <div class="container">
        <div class="row multitab-bg" style="background-image: url('{{ section.settings.bg_image | img_url: 'master' }}');">
            <div class="col-lg-4"></div>
            <div class="col-lg-8">
                <div class="multitab-right-section">
                    <h2>{{ section.settings.heading_text }}</h2>
                    <h4>{{ section.settings.sub_heading_text }}</h4>
                    <div class="multitab-wrap">
                        <div class="tabs-wrap">
                            <div class="multi-main-tabs">
                                <ul class="nav nav-tabs card-header-tabs" id="outerTab" role="tablist">
                                    <li class="nav-item">
                                        <a class="nav-link active" data-bs-toggle="tab" href="#tab-1" aria-controls="tab-1" role="tab" aria-expanded="true">
                                            <span class="svg-wrapper">
                                                {{- 'bullet-point.svg' | inline_asset_content -}}
                                            </span>
                                             {{ section.settings.tab_menu_1}}
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" data-bs-toggle="tab" href="#tab-2" aria-controls="tab-2" role="tab">
                                            <span class="svg-wrapper">
                                                {{- 'bullet-point.svg' | inline_asset_content -}}
                                            </span>
                                             {{ section.settings.tab_menu_2}}
                                        </a>
                                    </li>
                                </ul>
                            </div>

                            <div class="tabs-body tab-content">

                                <!-- Manual Tab -->
                                <div class="tab-pane active" id="tab-1" role="tabpanel">
                                    <!-- Nested Tab-1 -->
                                    <div class="card mt-3">
                                        <div class="tabs-menu">
                                            <h4>{{ section.settings.Select_Language}}</h4>
                                            <ul class="nav nav-tabs card-header-tabs" id="innerTab1" role="tablist">
                                                <li class="nav-item">
                                                    <a class="nav-link active" data-bs-toggle="tab" href="#tab-1-1" aria-controls="tab-1-1" role="tab" aria-expanded="true">{{ section.settings.Select_language_1 }}</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#tab-1-2" aria-controls="tab-1-2" role="tab">{{ section.settings.Select_language_2 }}</a>
                                                </li>
                                            </ul>
                                        </div>

                                        <div class="card-body tab-content">
                                            <!-- English Tab -->
                                            <div class="tab-pane active" id="tab-1-1" role="tabpanel">
                                                <div class="wrapper">
                                                    <ul class="tabs-dropdown-wrap">
                                                        {% for block in section.blocks %}
                                                            {% assign collection = collections[block.settings.category] %}
                                                            {% if block.type == "category_menu" %}
                                                                {% if collection and collection.products.size > 0 %}
                                                                    <li class="menu-title">
                                                                        <span class="">{{ section.settings.Select_Category }}</span>
                                                                    </li>
                                                                    <li class="menu-item menu-item-has-children">
                                                                        <span class="toggle-submenu">{{ collection.title }}</span>
                                                                        <div class="wrapper sub-wrap">
                                                                            <div class="menu-sub-title">
                                                                                <span class="">{{ collection.title }}</span>
                                                                            </div>
                                                                            <ul>
                                                                                {% for product in collection.products %}
                                                                                    {% if product.metafields.custom.download_file != blank %}
                                                                                        <li>
                                                                                            <a class="download" href="{{ product.metafields.custom.download_file | file_url }}" target="_blank">
                                                                                                {{ product.title }}
                                                                                                <span class="svg-wrapper download-wrapper">
                                                                                                    {{- 'download.svg' | inline_asset_content -}}
                                                                                                </span>
                                                                                            </a>
                                                                                        </li>
                                                                                    {% endif %}
                                                                                {% endfor %}
                                                                            </ul>
                                                                        </div>
                                                                    </li>
                                                                {% else %}
                                                                    <li>No products found in this collection.</li>
                                                                {% endif %}
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            </div>

                                            <!-- Dutch Tab -->
                                            <div class="tab-pane" id="tab-1-2" role="tabpanel">
                                                <div class="wrapper">
                                                    <ul class="tabs-dropdown-wrap">
                                                        {% for block in section.blocks %}
                                                            {% assign collection = collections[block.settings.category] %}
                                                            {% if block.type == "dutch_category_menu" %}
                                                                {% if collection and collection.products.size > 0 %}
                                                                    <li class="menu-title">
                                                                        <span class="">{{ section.settings.Select_Category }}</span>
                                                                    </li>
                                                                    <li class="menu-item menu-item-has-children">
                                                                        <span class="toggle-submenu">{{ collection.title }}</span>
                                                                        <div class="wrapper sub-wrap">
                                                                            <div class="menu-sub-title">
                                                                                <span class="">{{ collection.title }}</span>
                                                                            </div>
                                                                            <ul>
                                                                               <!-- Check if the Collection-Level Download Link Exists -->
                                                                              {% if collection.metafields.custom.collection_dutch_file != blank %}
                                                                                  <li>
                                                                                      <a class="download" href="{{ collection.metafields.custom.collection_dutch_file | file_url }}" target="_blank">
                                                                                          {{ block.settings.heading_download }} {{ collection.title }}
                                                                                          <span class="svg-wrapper download-wrapper">
                                                                                              {{- 'download.svg' | inline_asset_content -}}
                                                                                          </span>
                                                                                      </a>
                                                                                  </li>
                                                                              {% else %}
                                                                                  <!-- If No Collection-Level File, Show Product-Level Download Links -->
                                                                                  {% for product in collection.products %}
                                                                                      {% if product.metafields.custom.dutch_file != blank %}
                                                                                          <li>
                                                                                              <a class="download" href="{{ product.metafields.custom.dutch_file | file_url }}" target="_blank">
                                                                                                  {{ product.title }}
                                                                                                  <span class="svg-wrapper download-wrapper">
                                                                                                      {{- 'download.svg' | inline_asset_content -}}
                                                                                                  </span>
                                                                                              </a>
                                                                                          </li>
                                                                                      {% else %}
                                                                                          <li><span class="no-file">File not available for {{ product.title }}</span></li>
                                                                                      {% endif %}
                                                                                  {% endfor %}
                                                                              {% endif %}

                                                                            </ul>
                                                                        </div>
                                                                    </li>
                                                                {% else %}
                                                                    <li>No products found in this collection.</li>
                                                                {% endif %}
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- End Nested Tab-1 -->
                                </div>

                                <!-- Driver Tab -->
                                <div class="tab-pane" id="tab-2" role="tabpanel">
                                    <!-- Nested Tab-2 -->
                                    <div class="card mt-3">
                                        <div class="tabs-menu">
                                            <h4>{{ section.settings.Select_Operating_System}}</h4>
                                            <ul class="nav nav-tabs card-header-tabs" id="innerTab1" role="tablist">
                                                <li class="nav-item">
                                                    <a class="nav-link active" data-bs-toggle="tab" href="#tab-2-1" aria-controls="tab-2-1" role="tab" aria-expanded="true">{{ section.settings.Select_Operating_System_1 }}</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#tab-2-2" aria-controls="tab-2-2" role="tab">{{ section.settings.Select_Operating_System_2 }}</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#tab-2-3" aria-controls="tab-2-3" role="tab">{{ section.settings.Select_Operating_System_3 }}</a>
                                                </li>
                                            </ul>
                                        </div>

                                        <div class="card-body tab-content">
                                            <!-- Windows Tab -->
                                            <div class="tab-pane active" id="tab-2-1" role="tabpanel">
                                                 <div class="wrapper">
                                                    <ul class="tabs-dropdown-wrap">
                                                        {% for block in section.blocks %}
                                                            {% assign collection = collections[block.settings.windows_category] %}
                                                            {% if block.type == "driver_windows" %}
                                                                {% if collection and collection.products.size > 0 %}
                                                                    <li class="menu-title">
                                                                        <span class="">{{ section.settings.Select_Category }}</span>
                                                                    </li>
                                                                    <li class="menu-item menu-item-has-children">
                                                                        <span class="toggle-submenu">{{ collection.title }}</span>
                                                                        <div class="wrapper sub-wrap">
                                                                            <div class="menu-sub-title">
                                                                                <span class="">{{ collection.title }}</span>
                                                                            </div>
                                                                            <ul>
                                                                                {% for product in collection.products %}
                                                                                 {% if product.metafields.custom.digital_downloads_url != blank %}
                                                                                    <!-- Display the download link if the metafield is present -->
                                                                                    <a class="download" href="{{ product.metafields.custom.digital_downloads_url }}" target="_blank">
                                                                                      {{ product.title }}
                                                                                        <span class="svg-wrapper download-wrapper">
                                                                                            {{- 'download.svg' | inline_asset_content -}}
                                                                                        </span>
                                                                                    </a>
                                                                                  {% endif %}
                                                                                {% endfor %}
                                                                            </ul>
                                                                        </div>
                                                                    </li>
                                                                {% else %}
                                                                    <li>No products found in this collection.</li>
                                                                {% endif %}
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            </div>

                                           <!-- Mac Tab -->
                                        <div class="tab-pane" id="tab-2-2" role="tabpanel">
                                            <div class="wrapper">
                                                <ul class="tabs-dropdown-wrap">
                                                    {% for block in section.blocks %}
                                                        {% assign collection = collections[block.settings.mac_category] %}
                                                        {% if block.type == "driver_mac" %}
                                                            {% if collection and collection.products.size > 0 %}
                                                                <li class="menu-title">
                                                                    <span class="">{{ section.settings.Select_Linux_Driver }}</span>
                                                                </li>
                                                                <li class="menu-item menu-item-has-children">
                                                                    <span class="toggle-submenu">{{ collection.title }}</span>
                                                                    <div class="wrapper sub-wrap">
                                                                        <div class="menu-sub-title">
                                                                            <span class="">{{ collection.title }}</span>
                                                                        </div>
                                                                        <ul>
                                                                            {% for product in collection.products %}
                                                                                {% if product.metafields.custom.mac_file != blank %}
                                                                                    <li>
                                                                                        <a class="download" href="{{ product.metafields.custom.mac_file | file_url }}" target="_blank">
                                                                                            {{ product.title }}
                                                                                            <span class="svg-wrapper download-wrapper">
                                                                                                {{- 'download.svg' | inline_asset_content -}}
                                                                                            </span>
                                                                                        </a>
                                                                                    </li>
                                                                                {% endif %}
                                                                            {% endfor %}
                                                                        </ul>
                                                                    </div>
                                                                </li>
                                                            {% else %}
                                                                <li>No products found in this collection.</li>
                                                            {% endif %}
                                                        {% endif %}
                                                    {% endfor %}
                                                </ul>
                                            </div>
                                        </div>
                                          
                                           <!-- Linux Tab -->
                                                <div class="tab-pane" id="tab-2-3" role="tabpanel">
                                                    <div class="wrapper">
                                                        <ul class="tabs-dropdown-wrap">
                                                            {% for block in section.blocks %}
                                                                {% assign collection = collections[block.settings.linux_category] %}
                                                                {% if block.type == "driver_linux" %}
                                                                    {% if collection and collection.products.size > 0 %}
                                                                        <li class="menu-title">
                                                                            <span class="">{{ section.settings.Select_Linux_Driver }}</span>
                                                                        </li>
                                                                        <li class="menu-item menu-item-has-children">
                                                                            <span class="toggle-submenu">{{ collection.title }}</span>
                                                                            <div class="wrapper sub-wrap">
                                                                                <div class="menu-sub-title">
                                                                                    <span class="">{{ collection.title }}</span>
                                                                                </div>
                                                                                <ul>
                                                                                    {% for product in collection.products %}
