                                                                                            <li>
                                                                                                <a class="download" href="{{ product.metafields.custom.linux_file | file_url }}" target="_blank">
                                                                                                    {{ product.title }}
                                                                                                    <span class="svg-wrapper download-wrapper">
                                                                                                        {{- 'download.svg' | inline_asset_content -}}
